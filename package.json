{
  "name": "colorsnapper.com",
  "version": "0.0.0",
  "private": true,
  "description": "ColorSnapper Public Website",
  "main": "",
  "scripts": {
    "start": "gulp",
    "build": "node index.js",
    "clean": "rm -rf assets/ _site/",
    "deploy": "surge _site",
    "test": "echo \"Error: no test specified\" && exit 1",
    "production": "npm run clean && npm run build && npm run deploy",
    "staging": "npm run clean && npm run build && surge _site -d cs2-test.surge.sh",
    "imageoptim": "imageoptim --directory ./img -a -q && imageoptim --directory ./_images -a -q",
    "svgo": "svgo -f ./_images",
    "deps": "npm-check -s",
    "deps:update": "npm-check -u"
  },
  "author": "Andrey Okonetchnikov",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "babel-core": "^5.2.16",
    "babel-loader": "^5.0.0",
    "browser-sync": "^2.7.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.1",
    "gulp": "^3.8.11",
    "gulp-util": "^3.0.4",
    "handlebars": "^4.0.5",
    "imageoptim-cli": "^1.11.6",
    "metalsmith": "^2.1.0",
    "metalsmith-define": "^1.0.0",
    "metalsmith-in-place": "^1.3.3",
    "metalsmith-layouts": "^1.4.4",
    "metalsmith-markdownit": "^0.3.0",
    "metalsmith-permalinks": "^0.4.1",
    "metalsmith-webpack": "^1.0.3",
    "npm-check": "^4.1.4",
    "postcss-calc": "^5.2.0",
    "postcss-loader": "^0.8.0",
    "postcss-scss": "^0.1.3",
    "precss": "^1.4.0",
    "style-loader": "^0.13.0",
    "surge": "^0.17.7",
    "svgo": "^0.6.1",
    "url-loader": "^0.5.5",
    "webpack": "^1.8.4",
    "webpack-dev-server": "^1.8.0"
  }
}
