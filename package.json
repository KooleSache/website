{
  "name": "colorsnapper.com",
  "version": "0.0.0",
  "private": true,
  "description": "ColorSnapper Public Website",
  "main": "",
  "scripts": {
    "start": "gulp",
    "build": "NODE_ENV=production webpack -p && jekyll build --config _config_production.yml && rm -rf assets/",
    "clean": "rm -rf assets/ _site/",
    "deploy": "surge _site",
    "test": "echo \"Error: no test specified\" && exit 1",
    "production": "npm run clean && npm run build && npm run deploy",
    "staging": "npm run clean && npm run build && surge _site -d cs2-test.surge.sh",
    "imageoptim": "imageoptim --directory ./img -a -q && imageoptim --directory ./_images -a -q",
    "svgo": "svgo -f ./_images",
    "deps": "npm outdated --depth=0"
  },
  "author": "Andrey Okonetchnikov",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer-loader": "^1.2.0",
    "babel-core": "^5.2.16",
    "babel-loader": "^5.0.0",
    "browser-sync": "^2.7.1",
    "css-loader": "^0.12.0",
    "extract-text-webpack-plugin": "^0.7.1",
    "file-loader": "^0.8.1",
    "gulp": "^3.8.11",
    "gulp-util": "^3.0.4",
    "imageoptim-cli": "^1.11.6",
    "sass-loader": "^0.4.0",
    "style-loader": "^0.12.2",
    "surge": "^0.11.1",
    "svgo": "^0.5.1",
    "url-loader": "^0.5.5",
    "webpack": "^1.8.4",
    "webpack-dev-server": "^1.8.0"
  }
}
